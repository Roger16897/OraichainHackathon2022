<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>AI Oraichain Blockchain</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Assets/Admin/vendors/iconfonts/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/Assets/Admin/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Assets/Admin/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="~/Assets/Admin/images/favicon.png" />
    <link href="~/Content/dx.light.css" rel="stylesheet" />
    <link href="~/Content/dx.common.css" rel="stylesheet" />
    @RenderSection("Header", required: false)

</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar default-layout-navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
                <a class="navbar-brand brand-logo" href="index.html"><img src="~/Assets/Admin/images/logo.svg" alt="logo" /></a>
                <a class="navbar-brand brand-logo-mini" href="index.html"><img src="~/Assets/Admin/images/logo-mini.svg" alt="logo" /></a>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-stretch">
                <div class="search-field d-none d-md-block">
                    <form class="d-flex align-items-center h-100" action="#">
                        <div class="input-group">
                            <div class="input-group-prepend bg-transparent">
                                <i class="input-group-text border-0 mdi mdi-magnify"></i>
                            </div>
                            <input type="text" class="form-control bg-transparent border-0" placeholder="Search...">
                        </div>
                    </form>
                </div>
                <ul class="navbar-nav navbar-nav-right">
                    <li class="nav-item nav-profile dropdown">
                        <a class="nav-link dropdown-toggle" id="profileDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
                            <div class="nav-profile-img">
                                <img src="~/Assets/Admin/images/faces/login.png" alt="image">
                                <span class="availability-status online"></span>
                            </div>
                            <div class="nav-profile-text">
                                <p class="mb-1 text-black">@Session["TenDangNhap"]</p>
                            </div>
                        </a>
                        <div class="dropdown-menu navbar-dropdown" aria-labelledby="profileDropdown">
                            <a class="dropdown-item" href="#">
                                <i class="mdi mdi-cached mr-2 text-success"></i>
                                Quản lý tài khoản
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="/Login/Logout">
                                <i class="mdi mdi-logout mr-2 text-primary"></i>
                                Đăng xuất
                            </a>
                        </div>
                    </li>
                    <li class="nav-item d-none d-lg-block full-screen-link">
                        <a class="nav-link">
                            <i class="mdi mdi-fullscreen" id="fullscreen-button"></i>
                        </a>
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    @if (Session["type"].ToString() == "1")
                    {
                        <li class="nav-item nav-profile">
                            <a href="#" class="nav-link">
                                <div class="nav-profile-image">
                                    <img src="~/Assets/Admin/images/faces/admin.png" alt="profile">
                                    <span class="login-status online"></span>
                                </div>
                                <div class="nav-profile-text d-flex flex-column">
                                    <span class="font-weight-bold mb-2">Quản trị viên</span>
                                </div>
                                <i class="mdi mdi-bookmark-check text-success nav-profile-badge"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Index", "Home",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Index", "Home",new { Area = "Admin" })">
                                <span class="menu-title">Trang chủ</span>
                                <i class="mdi mdi-home menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false" aria-controls="ui-basic">
                                <span class="menu-title">Quản lý người dùng</span>
                                <i class="menu-arrow"></i>
                                <i class="mdi mdi-account-box menu-icon"></i>
                            </a>
                            <div class="collapse" id="ui-basic">
                                <ul class="nav flex-column sub-menu">
                                    <li class="nav-item  @(Url.Action("DanhSachNguoiDung", "User",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                                        <a class="nav-link" href="@Url.Action("DanhSachNguoiDung", "User",new { Area = "Admin" })">Danh sách người dùng</a>
                                    </li>
                                    <li class="nav-item @(Url.Action("GetController", "User",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                                        <a class="nav-link" href="@Url.Action("GetController", "User",new { Area = "Admin" })">Danh sách quyền</a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item @(Url.Action("Index", "LoaiDichVu",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Index", "LoaiDichVu",new { Area = "Admin" })">
                                <span class="menu-title">Quản lý loại dịch vụ</span>
                                <i class="mdi mdi-library-books menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Index", "NhaCungCapAI",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Index", "NhaCungCapAI",new { Area = "Admin" })">
                                <span class="menu-title">Quản lý nhà cung cấp AI</span>
                                <i class="mdi mdi-earth menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Index", "ChuyenGia",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Index", "ChuyenGia",new { Area = "Admin" })">
                                <span class="menu-title">Quản lý chuyên gia</span>
                                <i class="mdi mdi-account-key menu-icon"></i>
                            </a>
                        </li>
                    }
                    else if (Session["type"].ToString() == "3")
                    {
                        <li class="nav-item nav-profile">
                            <a href="#" class="nav-link">
                                <div class="nav-profile-image">
                                    <img src="~/Assets/Admin/images/faces/AI.jpg" alt="profile">
                                    <span class="login-status online"></span>
                                </div>
                                <div class="nav-profile-text d-flex flex-column">
                                    <span class="font-weight-bold mb-2">Nhà cung cấp AI</span>
                                </div>
                                <i class="mdi mdi-cloud-check text-success nav-profile-badge"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Home", "QuanLyNhaCCAI",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Home", "QuanLyNhaCCAI",new { Area = "Admin" })">
                                <span class="menu-title">Trang chủ</span>
                                <i class="mdi mdi-home menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="collapse" href="#general-pages" aria-expanded="false" aria-controls="general-pages">
                                <span class="menu-title">Quản lý dịch vụ</span>
                                <i class="menu-arrow"></i>
                                <i class="mdi mdi-format-list-bulleted-type menu-icon"></i>
                            </a>
                            <div class="collapse" id="general-pages">
                                <ul class="nav flex-column sub-menu">
                                    <li class="nav-item  @(Url.Action("DanhSachDichVu", "DichVu",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                                        <a class="nav-link" href="@Url.Action("DanhSachDichVu", "DichVu",new { Area = "Admin" })">Danh sách dịch vụ</a>
                                    </li>
                                    <li class="nav-item @(Url.Action("CauHinhThamSo", "DichVu",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                                        <a class="nav-link" href="@Url.Action("CauHinhThamSo", "DichVu",new { Area = "Admin" })">Cấu hình tham số</a>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item @(Url.Action("KetQuaAI", "QuanLyNhaCCAI",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("KetQuaAI", "QuanLyNhaCCAI",new { Area = "Admin" })">
                                <span class="menu-title">Danh mục kết quả</span>
                                <i class="mdi mdi-database menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Vi", "QuanLyNhaCCAI",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Vi", "QuanLyNhaCCAI",new { Area = "Admin" })">
                                <span class="menu-title">Quản lý ví</span>
                                <i class="mdi mdi-wallet-travel menu-icon"></i>
                            </a>
                        </li>

                    }

                    else
                    {
                        <li class="nav-item nav-profile">
                            <a href="#" class="nav-link">
                                <div class="nav-profile-image">
                                    <img src="~/Assets/Admin/images/faces/expert.png" alt="profile">
                                    <span class="login-status online"></span>
                                </div>
                                <div class="nav-profile-text d-flex flex-column">
                                    <span class="font-weight-bold mb-2">Chuyên gia</span>
                                </div>
                                <i class="mdi mdi-account-check text-success nav-profile-badge"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Home", "QuanLyCG",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Home", "QuanLyCG",new { Area = "Admin" })">
                                <span class="menu-title">Trang chủ</span>
                                <i class="mdi mdi-home menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("DanhSachYeuCau", "QuanLyCG",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("DanhSachYeuCau", "QuanLyCG",new { Area = "Admin" })">
                                <span class="menu-title">Danh sách yêu cầu</span>
                                <i class="mdi mdi-book-open-page-variant menu-icon"></i>
                            </a>
                        </li>
                        <li class="nav-item @(Url.Action("Vi", "QuanLyCG",new { Area = "Admin" }) == Request.Url.AbsolutePath ? "active" : "")">
                            <a class="nav-link" href="@Url.Action("Vi", "QuanLyCG",new { Area = "Admin" })">
                                <span class="menu-title">Quản lý ví</span>
                                <i class="mdi mdi-wallet-travel menu-icon"></i>
                            </a>
                        </li>
                    }


                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">1877 Team <i class="mdi mdi-heart text-danger"></i></span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Assets/Admin/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/Assets/Admin/vendors/js/vendor.bundle.addons.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/Assets/Admin/js/off-canvas.js"></script>
    <script src="~/Assets/Admin/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/Assets/Admin/js/dashboard.js"></script>
    <script src="~/Scripts/dx.all.js"></script>
    <script src="~/Scripts/aspnet/dx.aspnet.data.js"></script>
    @RenderSection("Scripts", required: false)

    <!-- End custom js for this page-->
</body>
</html>
